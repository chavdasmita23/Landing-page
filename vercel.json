{
    "version": 2,
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "builds": [
        {
          "src": "dist/index.js",
          "use": "@vercel/node"
        }
      ],
    "rewrites": [
      { "source": "/(.*)", "destination": "dist/index.js" }
    ]
  }